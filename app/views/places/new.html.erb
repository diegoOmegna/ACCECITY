<h1> places new </h1>

<% @place.errors.each do |error|%>
  <%= error.full_message %>
<% end %>

<%= simple_form_for @place do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :details %>
  <%= f.text_area :details, maxlength: 300 %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.input :header_photo, as: :file, input_html: { multiple: false } %>
  <%= f.submit %>
<% end %>

<%= render "shared/footer" %>
