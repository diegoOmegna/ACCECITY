<div class="container" >
  <div class="home-headers">
    <div class="salute-header">
      <% if user_signed_in? %>
        GoodDay <%= current_user.first_name %>!
      <% else %>
        Good Morning!
      <% end %>
    </div>
    <div class="main-header">
      <h1>Where do you want to go today?</h1>
    </div>
  </div>

  <%= render "shared/banner" %>

    <div class="container">
      <div class="button-center" >
        <%= link_to "+Add New Place", new_place_path, class: "btn btn-primary" %>
      </div>
    </div>
  <div class="top-places">
  Top Rated Places
  </div>
  <%= render "shared/home_card" %>
  <%= render "shared/footer"  %>
</div>
