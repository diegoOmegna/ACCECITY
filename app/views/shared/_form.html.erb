<%= form_with url: places_path, method: :get, class: "d-flex my-3" do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find a Place"
  %>
  <%= button_tag(type: "submit", class: "btn-form") do %>
    <i class="fa-solid fa-wheelchair-move"></i>
  <% end %>
<% end %>
