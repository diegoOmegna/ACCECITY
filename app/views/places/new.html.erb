<h1> places new </h1>

<% @place.errors.each do |error|%>
  <%= error.full_message %>
<% end %>

<%= form_with model: @place do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit %>
<% end %>
